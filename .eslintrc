 {
    "extends": ["eslint:recommended", "plugin:react/recommended"],
    "parser": "babel-eslint",
     "env": {
         "es6": true
     },
     "parserOptions": {
         "ecmaVersion": 6,
         "sourceType": "module",
         "ecmaFeatures": {
             "jsx": true
         }
     },
     "rules": {
         "semi": "error"
     }
}